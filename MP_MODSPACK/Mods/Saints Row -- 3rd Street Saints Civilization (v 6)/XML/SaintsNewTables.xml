<GameData>
	<Table name="SRTT_RespectLevels">
		<Column name="ID" type="integer" primarykey="true" autoincrement="true"/>
		<Column name="Type" type="text" notnull="true" unique="true"/>
		<Column name="RespectRequirement" type="integer" default ="0"/>
		<Column name="DummyBuilding" type="text" reference="Buildings(Type)"/>
		<Column name="DummyBuildingCount" type="integer" default="0"/>
		<Column name="PromotionUnitType" type="text" reference="Units(Type)"/>
		<Column name="Promotion" type="text" reference="UnitPromotions(Type)"/>
	</Table>
</GameData>